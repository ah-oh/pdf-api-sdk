{
    "name": "@ah-oh/pdf-sdk",
    "version": "0.0.1",
    "description": "",
    "keywords": [],
    "main": "dist/pdf-api-sdk.umd.js",
    "module": "dist/pdf-api-sdk.es5.js",
    "typings": "dist/types/pdf-api-sdk.d.ts",
    "files": [
        "dist"
    ],
    "author": "jannks <jan.kessler@ah-oh.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/ah-oh/pdf-api-sdk"
    },
    "license": "MIT",
    "engines": {
        "node": ">=6.0.0"
    },
    "scripts": {
        "test": "jest",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/test/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        },
        "collectCoverageFrom": [
            "src/*.{js,ts}"
        ]
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "jest": "^26.6.3",
        "jest-config": "^26.6.3",
        "prettier": "^2.2.1",
        "ts-jest": "^26.5.3",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "axios": "^0.21.1"
    }
}
